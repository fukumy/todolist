{
    "manifest_version": 3,
    "name": "Simple ToDo",
    "version": "1.0.0",
    "description": "A simple and beautiful ToDo application.",
    "action": {
        "default_popup": "index.html",
        "default_icon": "vite.svg"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "vite.svg",
        "48": "vite.svg",
        "128": "vite.svg"
    }
}